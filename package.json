{
  "name": "ssr-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "watch:dev": "NODE_ENV=development npm-run-all --parallel watch:server watch:client",
    "watch:prod": "NODE_ENV=production npm-run-all --parallel watch:server watch:client",
    "watch:server": "nodemon index.js",
    "watch:client": "webpack --watch --progress",
    "lint": "npm run lint:js; npm run lint:style",
    "lint:js": "eslint src --fix",
    "lint:style": "stylelint 'src/**/*.less'",
    "bundlesize": "bundlesize",
    "precommit": "npm run lint",
    "test": "jest src"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dotenv": "^6.1.0",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "isomorphic-fetch": "^2.2.1",
    "raf": "^3.4.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.26.0",
    "bundlesize": "^0.16.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "stylelint": "^9.5.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.17.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bundlesize": [
    {
      "path": "./dist/js/app.bundle.js",
      "maxSize": "170 kB"
    },
    {
      "path": "./dist/js/lib.bundle.js",
      "maxSize": "110 kB"
    }
  ],
  "author": "",
  "license": "MIT"
}
